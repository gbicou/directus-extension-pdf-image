{
	"name": "@bicou/directus-extension-pdf-image",
	"description": "Directus extension to create a screenshot image for each uploaded PDF document",
	"icon": "extension",
	"homepage": "https://github.com/gbicou/directus-extension-pdf-image",
	"author": "Benjamin VIELLARD <bicou@bicou.com>",
	"license": "MIT",
	"repository": "github:gbicou/directus-extension-pdf-image",
	"version": "0.0.5",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-hook",
		"pdf",
		"image"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "hook",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate",
		"test": "vitest run",
		"lint": "eslint"
	},
	"dependencies": {
		"pdf-to-img": "^4.5.0"
	},
	"devDependencies": {
		"@directus/api": "29.1.1",
		"@directus/extensions-sdk": "16.0.0",
		"@directus/sdk": "20.0.2",
		"@eslint/js": "9.34.0",
		"@stylistic/eslint-plugin": "5.2.3",
		"@types/node": "22.18.0",
		"dotenv": "17.2.1",
		"eslint": "9.34.0",
		"rollup-plugin-node-externals": "8.1.0",
		"typescript": "5.9.2",
		"typescript-eslint": "8.41.0",
		"vitest": "3.2.4"
	},
	"peerDependencies": {
		"@directus/api": "^29.1.1",
		"@directus/extensions-sdk": "^16.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
	"pnpm": {
		"onlyBuiltDependencies": [
			"argon2",
			"canvas",
			"esbuild",
			"isolated-vm",
			"sharp"
		],
		"ignoredBuiltDependencies": [
			"oracledb",
			"sqlite3"
		]
	}
}
